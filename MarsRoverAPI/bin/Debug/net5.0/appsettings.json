{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "Password": "",
    "AllowAdmin": true,
    "Ssl": false,
    "ConnectTimeout": 6000,
    "ConnectRetry": 2,
    "Database": 0,
    "ServiceName": "", // In case you are using Sentinel
    "Hosts": [
      {
        "Host": "redis", //localhost
        "Port": "6379"
      }
    ],
    "MaxValueLength": 1024,
    "PoolSize": 5,
    "KeyPrefix": ""
  },
  "MarsSurfaceRepository": {
    "UseRedis": true,
    "UseFile": false
  },
  "AppRedisConfiguration": {
    "MarsSurfaceRedisCacheKey": "mars-surface"
  }
}
